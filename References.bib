journal = {Journal of Fluid Mechanics},
mendeley-groups = {PhD thesis},
month = {mar},
pages = {125--134},
title = {{Air entrapment under an impacting drop}},
url = {http://www.journals.cambridge.org/abstract{\_}S0022112002003427},
volume = {478},
year = {2003}
}
