keywords = {air entrapment,breakup/coalescence,bubble ring,drop impact,drops and bubbles,high-speed imaging,splashing,vortex street,vorticity},
mendeley-tags = {air entrapment,bubble ring,drop impact,high-speed imaging,splashing,vortex street,vorticity},
month = {apr},
pages = {234--258},
title = {{Drop impact entrapment of bubble rings}},
